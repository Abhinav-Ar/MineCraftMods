command /msg [<player>] [<text>]:
	usage: /msg <player> <message>
	trigger:
		if arg-1 is online:
			set {convo::%player%} to arg-1
			set {convo::%arg-1%} to player
			send "&bFrom &d%{level::%player's uuid%}%♦ &7%player%&f: %arg-2%" to arg-1
			send "&bTo &d%{level::%arg-1's uuid%}%♦ &7%arg-1%&f: %arg-2%" to player
		else:
			send "&c&lERROR! &4| &cThis player isn't online!"

