on enter bound with id "portalunderworld":
	if {level::%player's uuid%} is greater than 24:
		if {underworldstory::%player's uuid%} = 1:
			teleport player to {underworldentrance}
			apply blindness to the player for 1 seconds
			play sound "entity.illusioner.mirror_move" with pitch 0.7 to player
		else:
			if {mainquest::%player's uuid%} = {book1}:
				apply blindness to the player for 3 seconds
				teleport player to {underworldentrance}
				play sound "entity.illusioner.mirror_move" with pitch 0.7 to player
				set {underworldstory::%player's uuid%} to 1
				setObjective(player, {talkfates}, {talkfatesquestbar})
	else:
		send "&cReach &dDivine Level 25&c to visit the underworld!" to player


