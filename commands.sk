variables:
	prefix = "&c&lERROR! &4|" #Change the prefix in chat
options:
	error.offline: "%{prefix}% &cThe player is not online"
	usage.kick: "%{prefix}% &cCorrect Usage: &b/kick <player> <reason>"
	usage.warn: "%{prefix}% &cCorrect Usage: &b/warn <player> <reason>"
	usage.mute: "%{prefix}% &cCorrect Usage: &b/mute <player> <time> <reason>"
	usage.unmute: "%{prefix}% &cCorrect Usage: &b/unmute <player> <reason>"
	usage.ban: "%{prefix}% &cCorrect Usage: &b/ban <player> <time> <reason>"
	usage.unban: "%{prefix}% &cCorrect Usage: &b/unban <player> <reason>"
	format.time: "&cThe time needs to be in the following format: <amount><sec/min/hour/day> (For example 10min)"
	usage.logs: "%{prefix}% &cCorrect Usage: &b/logs <player> &cor &b/logs server"
	command.cooldown: 5 seconds #Change the cooldown for the command

