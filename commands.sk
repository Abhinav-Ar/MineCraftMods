command /hat:
	permission: {@permissionhero}
	permission message: &c&lERROR! &4| &cYou need at least {@hero} &crank to use this command!
	trigger:
		if player's tool is air:
			if player's helmet is not air:
				send "&c&lERROR! &4| &cYou already have an item on your head!" to player
				stop
			else:
				send "&c&lERROR! &4| &cYou are not holding an item!" to player
				stop
		if player's helmet is not air:
			send "&c&lERROR! &4| &cYou already have an item on your head!" to player
			stop
		send "&a&lSUCSESS! &2| &fYou equipped %player's tool%" to player
		set player's helmet to player's tool
		remove player's tool from player's inventory

command /npc:luis:
  trigger:
    if {talkToLuis::%player%} is not set:
      if {luis::%player%} is not 1:
        set {luis::%player%} to 1
        send "&7[NPC] <##D3CF99>&lLuis&f: Hello my name is Luis." to player
        play sound "entity.evoker.ambient" to the player
        wait 3 seconds
        send "&7[NPC] <##D3CF99>&lLuis&f: I risked my life for this useless compass... It points to where you last died, but I never die!" to player
        play sound "entity.evoker.ambient" to the player
        wait 5 seconds
        send "&7[NPC] <##D3CF99>&lLuis&f: Do you want to buy it from me?" to player
        play sound "entity.evoker.ambient" to the player
        wait 3 seconds
        set {luis::%player%} to 0
        set {talkToLuis::%player%} to 0
        set {_gui} to a new chest inventory with 4 row with name "Luis's Shop &k&r"
        set slot (all integers between 0 and 35) of {_gui} to black stained glass pane named " "
        set slot 13 of {_gui} to recovery compass named "&fRecovery Compass" with lore "&fItem Ability: Death Location" and "&7Points to where you last" and "&7died. Used to be owned by" and "&7Luis." and " " and "&f&lCOMMON" and " " and "&7Cost" and "&61000 Gold" and " " and "&eClick to buy!"
        set slot 31 of {_gui} to barrier named "&c&lClose Menu" with all flags hidden
        open {_gui} to player
    else:
      set {_gui} to a new chest inventory with 4 row with name "Luis's Shop &k&r"
      set slot (all integers between 0 and 35) of {_gui} to black stained glass pane named " "
      set slot 13 of {_gui} to recovery compass named "&fRecovery Compass" with lore "&fItem Ability: Death Location" and "&7Points to where you last" and "&7died. Used to be owned by" and "&7Luis." and " " and "&f&lCOMMON" and " " and "&7Cost" and "&61000 Gold" and " " and "&eClick to buy!"
      set slot 31 of {_gui} to barrier named "&c&lClose Menu" with all flags hidden
      open {_gui} to player

command /anvil:
	permission: {@permissiongodly}
	aliases: /av
	permission message: &c&lERROR! &4| &cYou need at least {@godly} &crank to use this command!
	trigger:
		open anvil inventory for the player

command /pweather [<text>]:
	permission: {@permissionhero}
	aliases: /pw
	permission message: &c&lERROR! &4| &cYou need at least {@hero} &crank to use this command!
	trigger:
		if arg-1 is not set:
			send "&c&lERROR! &4| &cChoose from rainy, clear, or thunder. &b/pweather &l<weather>"
		if arg-1 is not "rainy" or "clear" or "thunder":
			send "&c&lERROR! &4| &cChoose from rainy, clear, or thunder. &b/pweather &l<weather>"
		else:
			set the client weather of player to (arg-1 parsed as weather type)
			send "&a&lSUCSESS! &2| &fYou changed your client-side weather to &9%arg-1%&f!" to player

command /trash:
	permission: {@permissionhero}
	permission message: &c&lERROR! &4| &cYou need at least {@hero} &crank to use this command!
	trigger:
		set {_trash} to a new chest inventory with 5 rows with name "Trash &4(Permanently Deleted!)"
		open {_trash} to player

command /wild:
	cooldown: 1 hour
	trigger:
		set {_locationx} to x-coord of player
		set {_locationz} to z-coord of player
		loop 4 times:
			set {_checkx} to x-coord of player
			set {_checkz} to z-coord of player
			send subtitle "&aTeleporting you to &2The Wild&a in %6 - loop-number% seconds" to player with fadein 0 second and fade out 0 second
			play sound "block.note_block.pling" to player
			if "%{_checkx}% %{_checkz}%" isn't "%{_locationx}% %{_locationz}%":
				send "&cTeleport cancelled. Do not move!" to player
				stop
			wait 20 ticks
		send subtitle "&aTeleporting you to &2The Wild&a in 1 second" to player with fadein 0 second and fade out 0 second
		apply potion of blindness of tier 1 to the player for 3 seconds
		apply potion of nausea of tier 1 to the player for 3 seconds
		play sound "entity.ender_dragon.flap" with volume 1 and pitch 0.5 to the player
		wait 1 second
		set {_x} to random number between -1000 and 1000
		set {_z} to random number between -1000 and 1000
		set {_loc} to highest block at location ({_x}, 0, {_z})
		teleport player to {_loc}
		send title "&aWelcome to &2The Wild&a!" with subtitle "&2Explore the world!" to player
		play sound "entity.player.levelup" to player
		wait 1 seconds
		play sound "entity.ender_dragon.flap" with volume 1 and pitch 0.5 to the player

