command /restartserver [<text>]:
	permission: op
	trigger:
		if arg-1 is not set:
			send "&c&LERROR &4| &cState a reason for restarting the server! &b/restart &l<reason>"
		else:
			loop all players:
				send title "&c&lSERVER RESTARTING!" with subtitle "&a%colored arg-1% &7(in &e60 &7seconds)" to loop-player for 3 seconds
				send "&c&lSERVER RESTARTING! &7(in &e60 &7seconds)" to loop-player
				play sound "block.note_block.pling" to loop-player
			execute console command "/save-all"
			wait 30 seconds
			loop all players:
				send title "&c&lSERVER RESTARTING!" with subtitle "&a%colored arg-1% &7(in &e30 &7seconds)" to loop-player for 3 seconds
				send "&c&lSERVER RESTARTING! &7(in &e30 &7seconds)" to loop-player
				play sound "block.note_block.pling" to loop-player
			wait 19 seconds
			set {untilrestart} to 10
			execute console command "/save-all"
			loop 10 times:
				loop all players:
					send title "&c&lSERVER RESTARTING!" with subtitle "&a%colored arg-1% &7(in &e%{untilrestart}% &7seconds)" to loop-player for 3 seconds
					send "&c&lSERVER RESTARTING! &7(in &e%{untilrestart}% &7seconds)" to loop-player
					play sound "block.note_block.pling" to loop-player
				wait 1 seconds
				subtract 1 from {untilrestart}
			loop all players:
				kick loop-player due to " %nl%&6&lLOSTGODS SMP%nl%&fServer is &crestarting...%nl%&FPlease rejoin in 1-2 minutes, your progress has been saved."
			execute console command "/stop"

